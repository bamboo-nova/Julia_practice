   "source": [
    "function pprocess_poisson(lambda)\n",
    "    xs = [0.0];\n",
    "\n",
    "    R = 0;\n",
    "    \n",
    "    x = 0.0;\n",
    "    #while x <= lambda*dt\n",
    "    while x <= 0.5\n",
    "        x = x + - log(rand()) / lambda;\n",
    "        push!(xs, round(x, digits=n));\n",
    "    end\n",
    "    \n",
    "    return xs\n",
    "\n",
    "end"
   ]
